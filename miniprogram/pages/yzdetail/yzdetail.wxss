
.list{
  top: 0;
  left: 0;
  width: 100%;
  height: 100;
}



.pro-box{
  position: absolute;
  left: 0;
  width: 100% ;
  height: 100%;
  overflow-y: auto;
  background-color: rgb(255, 255, 255);
}


.pro-item{
  padding: 20rpx;
  padding-bottom: 0rpx;
  height: 50px;
  border-bottom: 0.5rpx solid rgb(213, 224, 224);
}

.pro-text{
  width: 100%;
  margin-left: 20rpx;
  padding: 0%;
}

.title-name{
  font-size: 17px;
  color: #070000;
  width:max-content;
}
.title-date{
  font-size: 10px;
  color: #676870;
  width:max-content;
  margin-top: 10px;
}
